{
  "name": "react-app-shell",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "react-app-shell/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/react-app-shell",
        "index": "react-app-shell/src/index.html",
        "baseHref": "/",
        "main": "react-app-shell/src/main.tsx",
        "tsConfig": "react-app-shell/tsconfig.app.json",
        "assets": [
          "react-app-shell/src/favicon.ico",
          "react-app-shell/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "react-app-shell/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "react-app-shell/src/environments/environment.ts",
              "with": "react-app-shell/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "react-app-shell:build",
        "hmr": true,
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "react-app-shell:build:development"
        },
        "production": {
          "buildTarget": "react-app-shell:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "react-app-shell:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "react-app-shell/jest.config.ts"
      }
    }
  },
  "tags": []
}
